<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <!--<View-->
        <!--android:id="@+id/v_top_space"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="22.5dip"-->
        <!--android:background="#F8F8F9"-->
        <!--android:visibility="gone"/>-->

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dip"
        android:background="#DFDFE0"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="51.5dip"
        android:background="@drawable/selector_item_white"
        android:gravity="center_vertical"
        android:paddingLeft="20dip"
        android:paddingRight="20dip">

        <ImageView
            android:id="@+id/i_group"
            android:layout_width="10dip"
            android:rotation="90"
            android:layout_height="10dip"
            android:layout_marginRight="10dip"
            android:scaleType="centerInside"
            android:src="@mipmap/contact_group"/>

        <TextView
            android:id="@+id/t_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000"
            android:textSize="15sp"/>
    </LinearLayout>


</LinearLayout>